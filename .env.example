# [.env.example - STATUS: Not Started]
# This is a template for environment variables required by the Enterprise Profile Builder
# Copy this file to .env.local and fill in the actual values
# DO NOT commit .env.local to version control

# ============================================================================
# APPLICATION CONFIGURATION
# ============================================================================

# Application environment (development | staging | production)
VITE_APP_ENV=development

# Application URL (used for redirects and CORS)
VITE_APP_URL=http://localhost:5173

# ============================================================================
# ANTHROPIC CLAUDE API
# ============================================================================

# Anthropic API key for AI agent execution
# Get your API key from: https://console.anthropic.com/
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Claude model to use (claude-3-5-sonnet-20241022 recommended)
VITE_ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# ============================================================================
# SUPABASE CONFIGURATION (Optional - for backend features)
# ============================================================================

# Supabase project URL
VITE_SUPABASE_URL=https://your-project.supabase.co

# Supabase anonymous key (public key)
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ============================================================================
# ANALYTICS (Optional)
# ============================================================================

# Enable analytics tracking (true | false)
VITE_ANALYTICS_ENABLED=true

# Enable analytics debug mode (true | false)
VITE_ANALYTICS_DEBUG=false

# Google Analytics 4 Measurement ID (optional)
VITE_GA4_MEASUREMENT_ID=G-XXXXXXXXXX

# ============================================================================
# FEATURE FLAGS
# ============================================================================

# Enable dark mode feature (true | false)
VITE_FEATURE_DARK_MODE=false

# Enable advanced search (true | false)
VITE_FEATURE_ADVANCED_SEARCH=true

# Enable AI agent builder (true | false)
VITE_FEATURE_AGENT_BUILDER=true

# Enable integration marketplace (true | false)
VITE_FEATURE_INTEGRATIONS=true

# ============================================================================
# ERROR TRACKING (Optional)
# ============================================================================

# Sentry DSN for error tracking (optional)
VITE_SENTRY_DSN=your_sentry_dsn_here

# Sentry environment (development | staging | production)
VITE_SENTRY_ENVIRONMENT=development

# ============================================================================
# BUILD CONFIGURATION
# ============================================================================

# Enable source maps in production (true | false)
VITE_ENABLE_SOURCE_MAPS=false

# Enable build minification (true | false)
VITE_MINIFY=true

# ============================================================================
# NOTES
# ============================================================================

# 1. All VITE_ prefixed variables are exposed to the client-side code
# 2. Never commit actual API keys or secrets to version control
# 3. Use .env.local for local development (git ignored)
# 4. Use environment-specific files: .env.staging, .env.production
# 5. Refer to ENVIRONMENT_SETUP.md for detailed configuration instructions
